{
  "name": "@red-token/welshman",
  "version": "0.2.5",
  "license": "MIT",
  "description": "A collection of svelte stores for use in building nostr client applications.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    "./app": {
      "types": "./build/src/app/index.d.ts",
      "import": "./build/src/app/index.js",
      "require": "./build/src/app/index.js"
    },
    "./content": {
      "types": "./build/src/content/index.d.ts",
      "import": "./build/src/content/index.js",
      "require": "./build/src/content/index.js"
    },
    "./editor": {
      "types": "./build/src/editor/index.d.ts",
      "import": "./build/src/editor/index.js",
      "require": "./build/src/editor/index.js"
    },
    "./feeds": {
      "types": "./build/src/feeds/index.d.ts",
      "import": "./build/src/feeds/index.js",
      "require": "./build/src/feeds/index.js"
    },
    "./lib": {
      "types": "./build/src/lib/index.d.ts",
      "import": "./build/src/lib/index.js",
      "require": "./build/src/lib/index.js"
    },
    "./net": {
      "types": "./build/src/net/index.d.ts",
      "import": "./build/src/net/index.js",
      "require": "./build/src/net/index.js"
    },
    "./signer": {
      "types": "./build/src/signer/index.d.ts",
      "import": "./build/src/signer/index.js",
      "require": "./build/src/signer/index.js"
    },
    "./store": {
      "types": "./build/src/store/index.d.ts",
      "import": "./build/src/store/index.js",
      "require": "./build/src/store/index.js"
    },
    "./dvm": {
      "types": "./build/src/dvm/index.d.ts",
      "import": "./build/src/dvm/index.js",
      "require": "./build/src/dvm/index.js"
    },
    "./util": {
      "types": "./build/src/util/index.d.ts",
      "import": "./build/src/util/index.js",
      "require": "./build/src/util/index.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "predocs": "typedoc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npx typedoc && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "pub": "npm run lint && npm run build && npm publish",
    "build": "gts clean && tsc",
    "buildtest": "gts clean && tsc --noImplicitAny false",
    "lint": "gts lint",
    "format": "prettier --write .",
    "fix": "gts fix"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/red-token/welshman"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.5",
    "gts": "^6.0.2",
    "happy-dom": "^17.1.8",
    "mocha": "^10.7.3",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "^4.4.2",
    "typedoc-vitepress-theme": "^1.1.2",
    "typescript": "^5.6.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.5"
  },
  "files": [
    "build/**/*",
    "docs/**/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@braintree/sanitize-url": "^7.0.2",
    "@noble/curves": "^1.7.0",
    "@noble/hashes": "^1.6.1",
    "@red-token/welshman": "^0.2.4",
    "@scure/base": "^1.1.6",
    "@types/events": "^3.0.3",
    "@types/throttle-debounce": "^5.0.2",
    "@types/ws": "^8.5.13",
    "events": "^3.3.0",
    "fake-indexeddb": "^6.0.0",
    "fuse.js": "^7.0.0",
    "idb": "^8.0.0",
    "isomorphic-ws": "^5.0.0",
    "nostr-tools": "^2.7.2",
    "svelte": "^4.2.19",
    "throttle-debounce": "^5.0.2",
    "ws": "^8.17.1"
  },
  "peerDependencies": {
    "nostr-signer-capacitor-plugin": "^0.0.3"
  }
}
